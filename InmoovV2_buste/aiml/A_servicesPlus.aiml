<category><pattern>DONNE-MOI * INFORMATIONS SPORTIVE</pattern>
<template><think>
        <set name="topic">Information Sportive</set>
        <set name="nombre"><star index="1"/></set>
      </think>
      <condition>
		<li name="nombre" value="deux"><think><set name="nombre">2</set></think></li>

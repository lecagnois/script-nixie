<!--  Donne des informations depuis le site news api.org -->
<category><pattern>DONNE-MOI UNE INFORMATION SPORTIVE</pattern>
<template><set name="topic">Information Sportive</set>
    <oob>
        <mrl>
          <service>python</service>
